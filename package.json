{
  "name": "business-generator-ml-pipeline",
  "version": "1.0.0",
  "description": "AI-powered Business Name & Logo Generator with Machine Learning Pipeline",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd . && react-scripts start",
    "start:backend": "cd backend && python server.py",
    "train": "python train_pipeline.py",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "setup": "npm install && npm run install:backend"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "business-generator",
    "machine-learning",
    "ai",
    "logo-generator",
    "nlp",
    "transformer"
  ],
  "author": "Business Generator ML Team",
  "license": "MIT"
}
